{
  "name": "drizzle-admin",
  "version": "0.0.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build:all": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:css",
    "build:esm": "rollup -c ./rollup.esm.mjs",
    "build:cjs": "rollup -c ./rollup.cjs.mjs",
    "build:css": "@tailwindcss -i src/drizzle-ui/styles/globals.css -o dist/styles.css"
  },
  "dependencies": {
    "change-case-all": "^2.1.0",
    "clsx": "^2.1.1",
    "drizzle-zod": "^0.7.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.477.0",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tailwindcss/cli": "^4.0.9",
    "@types/react": "^19.0.10",
    "next": "^15.2.0",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.32.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-preserve-directives": "^1.1.3",
    "tailwindcss": "^4"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.40.0",
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "main": "dist/cjs/src/index.js",
  "exports": {
    "./components": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/types/components/index.d.ts"
    },
    "./routes": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/types/routes/index.d.ts"
    },
    "./types": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/types/types/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "module": "dist/esm/src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "description": "",
  "styles": "dist/styles.css"
}
