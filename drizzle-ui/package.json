{
  "name": "drizzle-ui",
  "version": "0.0.4",
  "private": false,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:all": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cli",
    "build:esm": "rollup -c ./rollup.esm.mjs",
    "build:cjs": "rollup -c ./rollup.cjs.mjs",
    "build:cli": "tsc --project ./tsconfig.cli.json && mkdir \"dist/cli/components/ui\" && cp -r components/ui dist/cli/components && cp -r cli/templates dist/cli",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@next/third-parties": "^15.1.6",
    "commander": "^13.1.0",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.474.0",
    "marked": "^15.0.6",
    "marked-highlight": "^2.2.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup": "^4.32.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-preserve-directives": "^1.1.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "peerDependencies": {
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "main": "dist/cjs/components/index.js",
  "module": "dist/esm/components/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "drizzle-ui": "dist/cli/index.js"
  },
  "description": "The minimalist component library used in Drizzle Next",
  "author": "travisluong",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  }
}
