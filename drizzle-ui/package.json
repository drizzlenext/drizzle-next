{
  "name": "drizzle-ui",
  "version": "0.0.7",
  "private": false,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:all": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cli && npm run build:css",
    "build:esm": "rollup -c ./rollup.esm.mjs",
    "build:cjs": "rollup -c ./rollup.cjs.mjs",
    "build:cli": "tsc --project ./tsconfig.cli.json && cp -r src dist && cp -r content dist",
    "clean": "rm -rf dist",
    "build:css": "@tailwindcss -i src/styles/styles.css -o dist/styles.css"
  },
  "dependencies": {
    "@next/third-parties": "^15.1.6",
    "@tiptap/core": "^2.11.5",
    "@tiptap/extension-code-block-lowlight": "^2.11.5",
    "@tiptap/extension-image": "^2.11.5",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/extension-text-align": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "commander": "^13.1.0",
    "lowlight": "^3.3.0",
    "next": "15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tailwindcss/cli": "^4.0.9",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/highlight.js": "^9.12.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.474.0",
    "marked": "^15.0.6",
    "marked-highlight": "^2.2.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup": "^4.32.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-preserve-directives": "^1.1.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "peerDependencies": {
    "next": "15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "main": "dist/cjs/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "module": "dist/esm/src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "drizzle-ui": "dist/cli/index.js"
  },
  "description": "The minimalist component library used in Drizzle Next",
  "author": "travisluong",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "styles": "dist/styles.css"
}
